    geoStatJoinChart({
      width: 300,
      height: 300,
      latLong: [-118.2428,34.00],
      scale: 18000,
      geoDataUrl: "./los-angeles-cities.json",
      statDataUrl: "./los-angeles-stats.csv"
    }, d3, topojson, _, queue);
  </script>

