	d3.csv("data.csv", function(error, data) { 
	
	var svg = d3.select("body")
					.append("svg")
					.attr("width",1000)
					.attr("height",1000);

	data.sort(function(a,b){
				return d3.descending(+a.value, +b.value);
			})

			var rects = svg.selectAll("rect")
								.data(data)
								.enter()
								.append("rect");

				rects.attr("x", 0)
					.attr("y", function(d, i) {
						return i * 25;
					})
					.attr("width", function(d) {
						return d.value/2;
					})
					.attr("height", 20)
					.attr("fill","#4DAAAB")

			var texts = svg.selectAll("text")
				.data(data)
				.enter()
				.append("text");

				texts.attr("x", function(d) {
					return d.value/2;
					})
					.attr("y", function(d, i) {
						return i * 25+15;
					})
					.attr("fill","#777777")
					.text(function(d) { return d.value;});

			var labels = svg.selectAll(".label")
				.data(data)
				.enter()
				.append("text")
				.attr("class","label");

