      shape-rendering: crispEdges;
		}
		
		.axis text {
         font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
         font-size: 18px;
         font-style: bold;
		}
      
      
    g.lines.highlight {
			opacity: 0.95;
  	}  

    g.lines.highlight circle {
	    fill: #ff0000;
      stroke: #ff0000;
		}  

