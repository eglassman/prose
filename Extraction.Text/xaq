<script type="text/javascript">




function myImps()
{
svg.selectAll("rect.pos1")
        .transition()
		.duration(2000)
        .attr("fill",function(d) {
			   		return p1icolor(d[2]);
			   })
			   
	svg.selectAll("rect.pos2")
        .transition()
		.duration(2000)
        .attr("fill",function(d) {
			   		return p2icolor(d[2]);
			   })		   
			   
		
}







function myClicks()
{
svg.selectAll("rect.pos1")
        .transition()
		.duration(2000)
        .attr("fill",function(d) {
			   		return p1ccolor(d[3]);
			   })
			   
	svg.selectAll("rect.pos2")
        .transition()
		.duration(2000)
        .attr("fill",function(d) {
			   		return p2ccolor(d[3]);
			   })		   
			   
 
			   


}





