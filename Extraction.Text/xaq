	  .attr("x", x[1]);
	
	labels.selectAll("text").filter(function(p) { return p.index == 314; }) // tommy doocey
	  .attr("dy", ".1em");
	
	labels.selectAll("text").filter(function(p) { return p.index == 327; }) // johnette marie foltz
	  .attr("x", x[0]);
    
