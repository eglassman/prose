    <div id="map" class="map"></div>
    <script type="text/javascript">
      var map = new ol.Map({
        target: 'map',
        layers: [
          new ol.layer.Tile({
            source: new ol.source.OSM()
          })
        ],
        view: new ol.View2D({
          center: ol.proj.transform([-58.4280189, -34.6051541], 'EPSG:4326', 'EPSG:3857'),
          zoom: 16
        })
      });
    </script>
