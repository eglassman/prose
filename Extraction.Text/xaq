<meta http-equiv="Content-Type" content="charset=utf-8"/>
<style type="text/css">
    body {
      font: 10px sans-serif;
    }

    .background path {
      fill: none;
      stroke: #ccc;
      stroke-opacity: .4;
      shape-rendering: crispEdges;
    }

    .brush .extent {
      fill: #000;
      fill-opacity: .2;
      stroke: #666;
      shape-rendering: crispEdges;
    }

    .axis line, .axis path {
      fill: none;
      stroke: #AAA;
      shape-rendering: crispEdges;
    }

    #firstChild .axis .tick  {
      display: none;
    }
</style>
<title></title>
