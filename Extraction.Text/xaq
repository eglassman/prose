    <script id="brushing">
var params = {
 "dom": "chart2290584f2d89",
"width": "800",
"height": "400",
"padding": {
 "top":     24,
"left":    100,
"bottom":     12,
"right":    100 
},
"color": d3.scale.category10(),
"id": "chart2290584f2d89",
"data": [
 {
 "since 2007":   0.47,
"tckrs": "VEA",
"2007":   -5.3,
"2008":  -44.9,
"2009":   40.2,
"2010":   17.1,
"2011":   -9.3,
"2012":   16.6,
"2013":   11.2,
"2014":    2.5 
},
{
 "since 2007":    1.9,
"tckrs": "EEM",
"2007":   21.3,
"2008":  -49.7,
"2009":   71.8,
"2010":   21.5,
"2011":   -6.3,
