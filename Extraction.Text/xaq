	  .attr("dy", "-.5em");
	
	labels.selectAll("text").filter(function(p) { return p.index == 306; }) // burt f. smith
	  .attr("dy", "-.5em");
	
	labels.selectAll("text").filter(function(p) { return p.index == 312; }) // wild conceits
	  .attr("dy", ".4em");
	
