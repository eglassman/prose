.data(series3)
.enter()
.append("svg:text")
.attr("class", "names")
.attr("x",20)
.attr("dy",10)
.append("svg:textPath")
.attr("startOffset",function(d) { return 0 })
.attr("xlink:href",function(d) { return "#"+d[5]; })
.text(function(d) { return d[2] })
.style("font-family","calibri")
.style("font-size","11px")
.style("font-weight",100)
.attr("fill","black")

	vis.selectAll(".enames")
.data(series4)
.enter()
.append("svg:text")
.attr("class", "enames")
.attr("x",20)
.attr("dy",8.5)
.append("svg:textPath")
.attr("startOffset",function(d) { return 0 })
.attr("xlink:href",function(d) { return "#"+d[5]; })
.text(function(d) { return d[2] })
