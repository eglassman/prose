  <script>    
    function setup(){
    
    	createCanvas(960, 500);
    }
    
    function update() {

    }
    
    function draw() {
      var y = Math.sin(frameCount/12) * 200;
      var x = Math.cos(frameCount/12) * 200;
      var size = Math.sin(frameCount / 6) * 100;
      ellipse(480 + x, 250 + y, size, size);
    	
    }
  </script>
