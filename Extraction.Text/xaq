            draw(document.getElementById("myRadius").value, document.getElementById("myX").value, document.getElementById("myY").value)
            return false;
        }

        function draw(rad, x, y){
            circle
            .style("fill", "red")
            .attr("r", rad)
            .attr("cx", x)
            .attr("cy", y)
        }

