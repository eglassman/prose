    <title>K-means</title>
    <style media="screen">
      body{
        margin: 0;
      }
      .point circle{
        fill:rgba(255, 255, 255, 1);
        stroke:#aaa;
        stroke-width:1px;
      }
      .lines line{
        stroke:#aaa;
        stroke-width:1px;
        stroke-opacity:.25;
      }
      .mean{
        opacity: 0;
        fill:rgba(255, 255, 255, .7);
        stroke-width:3px;
      }
    </style>
