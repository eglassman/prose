<style>

.node {
  font: 300 10px "Open Sans", "Helvetica Neue", Helvetica, Arial, sans-serif;
  fill: #bbb;
}

.node:hover {
  fill: #000;
}

.link {
  stroke: #00aeef;
  stroke-opacity: .4;
  fill: none;
  pointer-events: none;
}

.node:hover,
.node--source,
.node--target {
  font-weight: 700;
}

.node--source {
  fill: #e1d8ad;
}

.node--target {
  fill: #cf5c42;
}

.link--source,
.link--target {
  stroke-opacity: 1;
  stroke-width: 2px;
}

.link--source {
  stroke: #cf5c42;
}

.link--target {
  stroke: #e1d8ad;
}

