  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.10/angular.min.js"></script>
  <script src="http://d3js.org/d3.v3.min.js" charset="utf-8"></script>
  <style>
  html, body{
    width: 100%;
    height: 100%;
    font-size: 1em;
  }
  .sl {
    display: inline-block;
    width: 10em;
    vertical-align: middle;
    stroke: black;
    stroke-width: 1;
  }
  .sl path{
    fill: none;
  }
  .sl circle{
    fill: red;
  }
  .large{
    margin: 0;
    font-size: 3em;
