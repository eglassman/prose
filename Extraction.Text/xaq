      	//add the xAxis
      	var xAxis = d3.svg.axis()
          .scale(xScale)
          .orient("bottom")
          .ticks(10);

        svg.append("g")
        	.attr("class", "axis")
        	.attr("transform", "translate(0," + height + ")")
  		    .call(xAxis);

