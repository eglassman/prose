var $els = $('#a, #b, #c, #d, #e, #f, #g');

$.fn.domvertices.defaults = {append: $('.scene')};

$els.each(function (i, el) {
  $(el).domvertices().trace();
});
$(window).resize(function () {
  $.fn.domvertices.v.forEach(function (v) {
    v.update();
    v.trace();
