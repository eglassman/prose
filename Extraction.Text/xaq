  <script type="text/coffeescript">
  

svg = d3.select("body").append("svg")

circle = svg.selectAll("circle")
    .data([32, 57, 293], (d) -> d )

circle.enter().append("circle")
    .attr("cy", 60)
    .attr("cx", (d, i) -> (i * 100 + 30) )
    .attr("r", (d) -> Math.sqrt(d) )

circle.exit().remove()
    

  </script>
