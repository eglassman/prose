    .attr("width", width)
    .attr("height", height);

d3.json("/mbostock/raw/4090846/us.json", function(error, us) {
  if (error) throw error;

  svg.append("path")
      .datum(topojson.merge(us, us.objects.counties.geometries))
      .attr("class", "land")
      .attr("d", path);
