<script>
var width = 960;
var height = 500;

var svg = d3.select("body").append("svg")
	.attr("width", width)
	.attr("height", height);

d3.json("japan.geojson", function (error, jpn) {

    var projection = d3.geo.mercator()
	    .center([137, 34])
	    .scale(900)
	    .translate([width / 2, height / 2]);

    var path = d3.geo.path()
	    .projection(projection);

    svg.selectAll("path")
	.data(jpn.features)
	.enter()
	.append("path")
	.attr("stroke", "white")
	.attr("stroke-width", "0.5")
	.attr("d", path);
});
</script>
