<script>
 

var width = 1000, height = 1000;

var svg = d3.select("body").append("svg")
                              .attr("width", width)
                              .attr("height", height)
                              .attr("style", "border:1px solid black;");

 
var graph =  
    { 
    "nodes": [{ "name": "10", "isCandidate": 1,"isPCP": 0,"nodeLabel": "A"},
              { "name": "11", "isCandidate": 1,"isPCP": 0,"nodeLabel": "B"},
              { "name": "12", "isCandidate": 0,"isPCP": 0,"nodeLabel": "C"} 

             ],
    "links": [{ "source": "10","target": "11"},
              { "source": "10","target": "12" },
              { "source": "11","target": "12"} 
             ]
    };


//var graph = MiraouiEtAl2012;

var nodeMap = {};
graph.nodes.forEach(function(x) { nodeMap[x.name] = x; });
graph.links = graph.links.map(
                              function(x) {
                                            return {
                                                    source: nodeMap[x.source],
                                                    target: nodeMap[x.target] 
                                                  };
                                          }
                            );


var force = d3.layout.force()
                .size([width, height]);

force
    .charge(-1000)
    .nodes(graph.nodes)
    .linkDistance(200)
    .gravity(0.6)
    .links(graph.links)
    .on("tick", tick);

var link = svg.selectAll(".link")
                .data(graph.links)
                  .enter()
                  .append("line")
                    .attr("class", "link");
 

var circleGroups = svg.selectAll("node")
                        .data(force.nodes())
                          .enter()
                          .append("svg:g") ;

circleGroups.append("svg:circle")
              .attr("class", "regNode")
              .attr("r", 10)  ;

 var nodeTextPixSize = 5;
// both the text and background to make it easier to read
circleGroups.append("svg:text")
              .attr("x", function(d){return -0.5*(d.nodeLabel.length*nodeTextPixSize) ;})
              .attr("y", 0)
              .attr("class", "textShadow")
              .text(function(d) { return d.nodeLabel; });

circleGroups.append("svg:text")
              .attr("x", function(d){return -0.5*(d.nodeLabel.length*nodeTextPixSize) ;})
              .attr("y", 0)
              .attr("class", "textNotShadow")
              .text(function(d) { return d.nodeLabel; });
 
function tick() {

  circleGroups.attr("transform", function(d) { return "translate(" + d.x + "," + d.y + ")";});

    link.attr("x1", function(d) { return d.source.x; })
        .attr("y1", function(d) { return d.source.y; })
        .attr("x2", function(d) { return d.target.x; })
        .attr("y2", function(d) { return d.target.y; });

};

svg.selectAll(".link").remove();


var burnIn = 200;
force.start();
for (var i = burnIn * burnIn; i > 0; --i) force.tick();
force.stop();

//post simulate coordinates fix
 
// A
graph.nodes[0].x = 50;
graph.nodes[0].px = 50;
graph.nodes[0].y = 450;
graph.nodes[0].py = 450;
// B
graph.nodes[1].x = 50;
graph.nodes[1].px = 50;
graph.nodes[1].y = 50;
graph.nodes[1].py =50;
// C
graph.nodes[2].x = 450;
graph.nodes[2].px = 450;
graph.nodes[2].y = 450;
graph.nodes[2].py = 450;
 


//render order: links, collapsed nodes, and then significant nodes
link.attr("x1", function(d) { return d.source.x; })
    .attr("y1", function(d) { return d.source.y; })
    .attr("x2", function(d) { return d.target.x; })
    .attr("y2", function(d) { return d.target.y; });

circleGroups.attr("transform", function(d) { return "translate(" + d.x + "," + d.y + ")";});


//////////////////////////////////////////////////////////////////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////////////////////////////////////////
/////////////////////////////////////           FDEB        //////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////////////////////////////////////////////////

//////////////////////////////////////////////////////
//               Helper Functions                   //
//////////////////////////////////////////////////////

// so the edge name key is a combination of the original indices
// such that the larger index is always first in the key
function getEdgeName(e){

  var srcNameInt = parseInt(e.source.name);
  var targNameInt = parseInt(e.target.name);

  //sort the original indices of each node and make a key: "greaterIndex-smallerIndex"
  var firstPart = srcNameInt > targNameInt ? srcNameInt : targNameInt;
  var secondPart = srcNameInt < targNameInt ? srcNameInt : targNameInt;

  // so the key is a combination of the original index of each node where the larger index is always first in the key
  var edgeName = firstPart.toString() + "-" + secondPart.toString();

  return edgeName;
}

function midPoint(p1,p2){
  if(p1<p2){
    return (p1 + 0.5*(p2-p1));
  }else{
    return (p2 + 0.5*(p1-p2));
  } 
}

function midPointBetweenPoints(p1,p2){
  var Pm = {x:0, y:0};

  if(p1.x < p2.x){
    Pm.x = (p1.x + 0.5*(p2.x-p1.x));
  }else if (p1.x >= p2.x){
    Pm.x = (p2.x + 0.5*(p1.x-p2.x));
  }

  if(p1.y < p2.y){
    Pm.y = (p1.y + 0.5*(p2.y - p1.y));
  }else if (p1.y >= p2.y){
    Pm.y = (p2.y + 0.5*(p1.y - p2.y));
  }

  return Pm;

}

function orderAsc(p1,p2){
  if(p1<p2){
    return [p1,p2];
  }
  else{
    return [p2,p1];
  } 
}

function dist(p1,p2){
  return Math.abs(p1-p2);
}

function distBetweenPoints(p1,p2){
  return Math.sqrt( squared(p1.x-p2.x) + squared(p1.y-p2.y) );
}

function squared(x){
  return Math.pow(x,2);
}

//////////////////////////////////////////////////////
//         Compatibility Measure Functions          //
//////////////////////////////////////////////////////
//angle compatibility
function angleCompat(P,Q){

    var Ca;
    var dotProduct = (P.x2 - P.x1) * (Q.x2 - Q.x1) + (P.y2 - P.y1) * (Q.y2 - Q.y1);

    if (directionAffectsCompat){
        Ca = (dotProduct / (P.length*Q.length)+1) / 2.0;
    }else{
        Ca = Math.abs(dotProduct / (P.length*Q.length));
    }

    if (Math.abs(Ca) < epsilon)
    {
        Ca = 0.0;
    }
    if (Math.abs(Math.abs(Ca) - 1.0) < epsilon)
    {
        Ca = 1.0;
    }

    return Ca;

}

//scale compatibility
function scaleCompat(P,Q){

    var aveLength = 0.5*(P.length+Q.length);
    var minOfPQ = Math.min(P.length, Q.length);
    var maxOfPQ = Math.max(P.length, Q.length);

    var Cs = 2 / ( (aveLength/minOfPQ) + (maxOfPQ/aveLength) );

    return Cs;
}

//position compatibility
function posCompat(P,Q){
    var aveLength = 0.5*(P.length+Q.length);
    var Pm = [midPoint(P.x2,P.x1), midPoint(P.y2,P.y1)];
    var Qm = [midPoint(Q.x2,Q.x1), midPoint(Q.y2,Q.y1)];
    
    var distBetweenMidpoints = Math.sqrt( squared(Pm[0] - Qm[0]) + squared(Pm[1] - Qm[1]) );

    var Cp = aveLength/(aveLength + distBetweenMidpoints);
    return Cp;
}

//visibility compatibility
function vizCompat(P,Q){

    var Cv = 1;

    var vPQ = viz(P,Q);
    var vQP = viz(Q,P);

    Cv = Math.min( vPQ, vQP );

    if (angleCompat(P,Q) * scaleCompat(P,Q) * posCompat(P,Q) > .9){
        //Cv = Math.min( viz(P,Q), viz(Q,P) );
    }
    else{
        //Cv = 1.0;
    }

    return Cv;
}


function projectPointToLine(x1, y1, x2, y2, x, y, np){
    var L = Math.sqrt( squared(x1-x2) + squared(y1-y2) );
    var r = ((y1-y)*(y1-y2) - (x1-x)*(x2-x1)) / (L * L);
        
    np.x = x1 + r * (x2-x1);
    np.y = y1 + r * (y2-y1);
    return np;
}

function viz(P,Q){

    var returnValue, ratio, Im, 
        I0 = {x:undefined,y:undefined}, 
        I1 = {x:undefined,y:undefined},
        Pm = {x: midPoint(P.x1,P.x2), y: midPoint(P.y1,P.y2)},
        P0 = {x:P.x1,y:P.y1},
        P1 = {x:P.x2,y:P.y2},
        Q0 = {x:Q.x1,y:Q.y1},
        Q1 = {x:Q.x2,y:Q.y2};

    I0 = projectPointToLine(P0.x, P0.y, P1.x, P1.y, Q0.x, Q0.y, I0);
    I1 = projectPointToLine(P0.x, P0.y, P1.x, P1.y, Q1.x, Q1.y, I1);
    Im = midPointBetweenPoints(I0, I1);

    var dPmIm = distBetweenPoints(Pm,Im);
    var dI0I1 = distBetweenPoints(I0,I1);

    ratio = 2 * dPmIm / dI0I1;

    if (isFinite(ratio)) returnValue = Math.max(1-ratio,0);
    else returnValue = 0;

  return returnValue;
}

function totalEdgeCompat(P,Q){
  return (angleCompat(P,Q) * scaleCompat(P,Q) * posCompat(P,Q));
}

//////////////////////////////////////////////////////
//         Parameters of the FDEB Algorithm         //
//////////////////////////////////////////////////////

var barnesHutTheta = 0.6,//Barnes–Hut approximation criterion;
    allSubDivPoints = [],
    subDivPointsPerEdge = 10,
    springConstant = 0.1, //global spring constant
    numSegments = subDivPointsPerEdge+1, 
    localSpringConstant;

    edgeLookUp = [],
    numEdges = graph.links.length,

    numSteps = 10,
    stepSize = 2,
    epsilon = 1e-7,
    directionAffectsCompat = false;

var  q, i, o;

var angleCompLookup = {};
var scaleCompLookup = {};
var positionCompLookup = {};
var vizCompLookup = {};
var totalCompLookup = {};
var cardinal = d3.svg.line()
                       .x(function(d) { return d.x; })
                       .y(function(d) { return d.y; })
                        .interpolate("cardinal");

///////////////////////////////////////////////////////////
// Initialize the subdivisionPoints array with just the  //
// start and end fixed points for each edge              //
///////////////////////////////////////////////////////////

function initSubDivPoints(edges){

      for (var edgeIndex =  0; edgeIndex < edges.length; edgeIndex++) {

        var e = edges[edgeIndex];

        var edgeName = getEdgeName(e);

        if (typeof edgeLookUp[edgeName] == 'undefined'){

          edgeLookUp[edgeName] = {x1: e.source.x,
                                  y1: e.source.y,
                                  x2: e.target.x,
                                  y2: e.target.y,
                                  length: distBetweenPoints(e.source,e.target)
                                };

        }
       

        var dx = dist(e.source.x,e.target.x) / (subDivPointsPerEdge+1);
        var dy = dist(e.source.y,e.target.y) / (subDivPointsPerEdge+1);

        var sourceXIsLess = e.source.x < e.target.x ? true: false;
        var sourceYIsLess = e.source.y < e.target.y ? true: false;

        e.subdivisionPoints =  [];

        //now push the four intermeduate points
        for (var segIndex = 1; segIndex < subDivPointsPerEdge+1; segIndex++) {
           
           //the source and target at in 1 of 4 possible states with [[src, ],[0,trg]] in matrix and go clockqise until [[ , trg],[src,]]
           if (sourceXIsLess && sourceYIsLess){

              e.subdivisionPoints.push( { "edgeName": edgeName,
                                          "subDivIndex": segIndex,
                                          "x": e.source.x + segIndex*dx,
                                          "px": e.source.x + segIndex*dx, 
                                          "y": e.source.y + segIndex*dy,
                                          "py": e.source.y + segIndex*dy
                                          });

           }else if (!sourceXIsLess && !sourceYIsLess){

              e.subdivisionPoints.push( { "edgeName": edgeName,
                                          "subDivIndex": segIndex,
                                          "x": e.target.x + segIndex*dx,
                                          "px": e.target.x + segIndex*dx, 
                                          "y": e.target.y + segIndex*dy,
                                          "py": e.target.y + segIndex*dy
                                          });   

           }else if (!sourceXIsLess && sourceYIsLess){

              e.subdivisionPoints.push( { "edgeName": edgeName,
                                          "subDivIndex": segIndex,
                                          "x":  e.target.x + segIndex*dx,
                                          "px": e.target.x + segIndex*dx, 
                                          "y":  e.target.y - segIndex*dy,
                                          "py": e.target.y - segIndex*dy
                                          }); 

           }else if (sourceXIsLess && !sourceYIsLess){

              e.subdivisionPoints.push( { "edgeName": edgeName,
                                          "subDivIndex": segIndex,
                                          "x":  e.source.x + segIndex*dx,
                                          "px": e.source.x + segIndex*dx, 
                                          "y":  e.source.y - segIndex*dy,
                                          "py": e.source.y - segIndex*dy
                                          });
           }
        }

        if (sourceXIsLess){
              //insert in first position
              e.subdivisionPoints.splice(0, 0, { "edgeName": edgeName,
                                                 "subDivIndex": 0,
                                                 "fixed":true,
                                                 "x": e.source.x,
                                                 "px": e.source.x, 
                                                 "y": e.source.y,
                                                 "py": e.source.y
                                                });
              e.subdivisionPoints.push({ "edgeName": edgeName,
                                         "subDivIndex": subDivPointsPerEdge+1,
                                         "fixed":true,
                                         "x": e.target.x,
                                         "px": e.target.x, 
                                         "y": e.target.y,
                                         "py": e.target.y
                                       });

        }else{
              //insert in first position
              e.subdivisionPoints.splice(0, 0, { "edgeName": edgeName,
                                                 "subDivIndex": 0,
                                                 "fixed":true,
                                                 "x": e.target.x,
                                                 "px": e.target.x, 
                                                 "y": e.target.y,
                                                 "py": e.target.y
                                                });
              e.subdivisionPoints.push({ "edgeName": edgeName,
                                         "subDivIndex": subDivPointsPerEdge+1,
                                         "fixed":true,
                                         "x": e.source.x,
                                         "px": e.source.x, 
                                         "y": e.source.y,
                                         "py": e.source.y
                                       });
        }

        //just holds a reference to the subdivisionPoints
        edgeLookUp[edgeName].subdivisionPoints = e.subdivisionPoints;

        for (i = 0; i < e.subdivisionPoints.length; i++) {
          if (!e.subdivisionPoints[i].fixed){
              allSubDivPoints.push(e.subdivisionPoints[i]);
          } 
        }
  }

}

function initEdgeCompatLookups(edges){

    for (var i =  0; i < edges.length; i++) {

          var edgeNamei = getEdgeName(edges[i]);

          angleCompLookup[edgeNamei] = {};
          scaleCompLookup[edgeNamei] = {};
          positionCompLookup[edgeNamei] = {};
          vizCompLookup[edgeNamei] = {};
          totalCompLookup[edgeNamei] = {};

          for (var j =  i+1; j < edges.length; j++) {

            var edgeNamej = getEdgeName(edges[j]);

            var P = edgeLookUp[edgeNamei];
            var Q = edgeLookUp[edgeNamej];

            angleCompLookup[edgeNamei][edgeNamej]    = angleCompat(P,Q);
            scaleCompLookup[edgeNamei][edgeNamej]    = scaleCompat(P,Q);
            positionCompLookup[edgeNamei][edgeNamej] = posCompat(P,Q);
            vizCompLookup[edgeNamei][edgeNamej]      = vizCompat(P,Q);
            totalCompLookup[edgeNamei][edgeNamej]    = totalEdgeCompat(P,Q);
          }
    }

}

function attract(node) {

  return function(quad, x1, _, x2) {

    if (quad.point!=null && (quad.point.edgeName != node.edgeName) && !quad.point.fixed) {

        var dx = quad.cx - node.x,
            dy = quad.cy - node.y, 
            dn = 1/Math.sqrt(dx * dx + dy * dy);


          var totalEdgeCompatibility = totalCompLookup[node.edgeName][quad.point.edgeName];

          //if the lookup doesn't exist then it's the opposite order
          if (typeof totalEdgeCompatibility == 'undefined') 
            totalEdgeCompatibility = totalCompLookup[quad.point.edgeName][node.edgeName];
          
          var k = totalEdgeCompatibility * dn * dn;


        if ((x2 - x1) * dn < barnesHutTheta) {
            node.Fei_x += (dx*k);
            node.Fei_y += (dy*k);
          return true;
        }

        if (quad.point && isFinite(dn)) {
            node.Fei_x += (dx*k);
            node.Fei_y += (dy*k);
        }
    }

    return false;
  };
}

//compute center of mass for each quad tree node
function centerOfMass(quad) {

    var cx = 0, 
        cy = 0;

    quad.totPoints = 0;

    if (!quad.leaf) {

        var nodes = quad.nodes, 
                n = nodes.length, 
                i = -1, 
                c;

        while (++i < n) {
          c = nodes[i];
          if (c == null) continue;
          //recursion
          centerOfMass(c);
          //add the number of points in child node to total points
          quad.totPoints += c.totPoints;
          cx += c.cx;
          cy += c.cy;
        }

    }

    if (quad.point) {
        //make sure no coincident nodes
        if (!quad.leaf) {
          quad.point.x += Math.random() - .5;
          quad.point.y += Math.random() - .5;
        }
        if (!quad.point.fixed){
          quad.totPoints +=1;
          cx += quad.point.x;
          cy += quad.point.y;
        }
    }

    //finally add the center of mass for the node
    quad.cx = cx / quad.totPoints;
    quad.cy = cy / quad.totPoints;

}

function renderInitBundledEdges(edges){

       var cardinalPath = svg.selectAll("path")
                                .data(edges)
                                .enter()
                                .append("path")
                                .attr("class", "fdebPath")
                                 .attr("d", function(d){ return cardinal(d.subdivisionPoints)})
                                  .attr("stroke", "red")
                                  .attr("stroke-width", 0.25)
                                  .attr("fill", "none");
}

function fdebStep(){


      q = d3.geom.quadtree(allSubDivPoints);

      centerOfMass(q);

      i = -1;
      while (++i < allSubDivPoints.length) {

          if(!(o = allSubDivPoints[i]).fixed){
              o.Fei_x = 0;
              o.Fei_y = 0;
              q.visit(attract(o));
          }

      }

      i = -1;
      while (++i < allSubDivPoints.length) {
        
          o = allSubDivPoints[i];

          //keep the two endpoints fixed
          if (o.fixed) {
              o.x = o.px;
              o.y = o.py;
          } 
          else{ 

              o.Fpi_x = o.Fei_x //- o.Fsi_x;
              o.Fpi_y = o.Fei_y //- o.Fsi_y;

              o.px += o.Fei_x;
              o.py += o.Fei_y;

              //calculate the distance of the original vector u
              var distOfu = Math.sqrt(squared(o.Fpi_x)+squared(o.Fpi_y));
              
              //unit vector adjustment
              var dx = (o.Fpi_x)/distOfu;
              var dy = (o.Fpi_y)/distOfu;

              o.x = o.x + dx;
              o.y = o.y + dy;

          }
      }
}

function runThisManyIterationsOfFDEB(numIterations){
      var fIter = -1;
      while (++fIter < numIterations) {
         fdebStep()
      }
}

function updateFDEBedges(edges){

   var cardinalPath = svg.selectAll(".fdebPath")
                          .data(edges)
                           .transition()
                           .duration(2000)
                           .attr("d", function(d){ return cardinal(d.subdivisionPoints)})
                            .attr("stroke", "red")
                            .attr("stroke-width", 0.5)
                            .attr("fill", "none");
}


function FDEB(edges, numIter){

    initSubDivPoints(edges);
    initEdgeCompatLookups(edges);

    renderInitBundledEdges(edges);
    runThisManyIterationsOfFDEB(numIter);
    updateFDEBedges(edges);

}

FDEB(graph.links, 100);


</script>

</body> 
</html><!DOCTYPE html>
