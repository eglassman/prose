      }

      .ball-group:hover .btn-close{
        opacity: 1;
        cursor: pointer;
      }

      .ball-group:active .btn-close{
        display: none;
        opacity: 0;
      }
      .ball-group:active .btn-close:active{
        display: block;
        opacity: 1;
