		<script type="text/javascript">		

			var wid=800, hei=600, pad=20,
					padmid=70, wr=100, widbar=600 ;
			
			var players, playlong, rank ;
			
			// graphic containers
			var svg = d3.select("body")
						.append("svg")
						.attr("width", wid+3*pad+wr)
						.attr("height", hei+3*pad+padmid);
			
			var gbar = svg.append("g")
						.attr("transform","translate("+(3*pad)+","+pad+")");
			var gtime = svg.append("g")
						.attr("transform","translate("+pad+","+(padmid+pad+hei/2)+")");
			var gleg = svg.append("g")
						.attr("transform","translate("+(2*pad+wid)+","+(pad)+")");
			
			// graphic elements
			var boules, playlines, barpl ;	
			var gtitle ;
			
			// Scales and axes			
			var xscale = d3.scale.linear().range([0,wid]).domain([1999,2016]);
			var yscale = d3.scale.linear().range([0,hei/2])
							.domain([0,13]);
			var legscale = d3.scale.linear().range([0,hei])
							.domain([0,14]);
							
			var barxscale = d3.scale.ordinal()
								.rangeBands([0,widbar]) ;
							
			var xaxis = d3.svg.axis()
							.scale(xscale)
							.orient("bottom")
							.tickFormat(d3.format(".0f"));							

			var yaxis = d3.svg.axis()
							.scale(yscale)
							.orient("right");
							
			var namesax = d3.svg.axis()
							.scale(barxscale)
							.orient("bottom");

			var colour = d3.scale.category20();
			
			// 				
			var nbYears = 13 ;
			var nbPlay = 14 ;
				
			var lines = d3.svg.line()
						.x(function(d) { return xscale(d.Year); })
						.y(function(d) { return yscale(d.plac); });
						
			d3.csv("Suri3.csv",function(ll) {			
				rank=ll ;
			d3.csv("Suri2.csv",function(datalong) {			
				playlong = datalong ;
			d3.csv("Suri.csv",function(data) {
				// extracting names of players
				// and setting input values for ordinal scales
				colour.domain(d3.keys(data[0]).filter(function(key){return key !== "Year";}));
				barxscale.domain(d3.keys(data[0]).filter(function(key){return key !== "Year";}));
			
				players = colour.domain().map(function(name){
					return {name: name,
							values: data.map(function(d){
								return {Year:d.Year, plac:+d[name]};
								})
							};
				});
				
				
				// bar graph
				barpl = gbar.selectAll("rect")
								.data(playlong)
								.enter()
								.append("rect")
									.attr("class","pbars");
				barpl.attr("x",function(d){return barxscale(d.short);})
					.attr("y",function(d){
						var yy ;
						if (d.plac==13 || d.Year!=2015 ) {yy=hei/2}
						else {yy=yscale(d.plac)}
						return yy;})
					.attr("width",0.9*barxscale.rangeBand())
					.attr("height",function(d){
						var hh;
						if (d.plac==13 || d.Year!=2015) {hh=0}
						else {hh=hei/2-yscale(d.plac)}
						return hh ;})
					.attr("fill",function(d){return colour(d.short)})
					.append("title")
						.text(function(d){
							return d.short +": "+d.plac});
							
				// add title
				gtitle = gbar.append("text")
					.attr("x",pad+widbar/2)
					.attr("y",pad)
					.attr("text-anchor","middle")
					.text("Placings in 2015")
				
				// time line graph
				playline = gtime.selectAll(".players")
									.data(players)
									.enter()
									.append("g")
										.attr("class","players");
										
				playline.append("path")
							.attr("class","line")
							.attr("d",function(d) {return lines(d.values);})
							.attr("stroke",function(d){return colour(d.name);})
							.attr("opacity",0.7);
							
				var labels = gleg.selectAll(".labs")
								.data(rank)
								.enter()
								.append("text")
									.attr("class","labs") ;
				labels.attr("x",2*pad)
						.attr("y",function(d){
							return legscale(d.rank);})
						.text(function(d){return d.name;})
						
				var labrec = gleg.selectAll("line")
								.data(rank)
								.enter()
								.append("line")
									.attr("x1",0)
									.attr("x2",1.8*pad)
									.attr("y1",function(d){return (legscale(d.rank)-5);})
									.attr("y2",function(d){return (legscale(d.rank)-5);})
									.attr("stroke",function(d){return colour(d.name);}) 
									.attr("stroke-width",2);
									
				var labcir = gleg.selectAll("circle")
								.data(rank)
								.enter()
								.append("circle")
									.attr("cx",(pad*0.9))
									.attr("cy",function(d){return (legscale(d.rank)-5);})
									.attr("fill",function(d){return colour(d.name);}) 
									.attr("r",7);									
						
				boules = gtime.selectAll("circle")		
								.data(playlong)
								.enter()
								.append("circle");
				boules.attr("cx",function(d){return xscale(d.Year);})
						.attr("cy",function(d){return yscale(d.plac);})
						.attr("r",7)
						.attr("fill",function(d){return colour(d.short);})
						.attr("opacity",function(d){return d.plac==13?0:0.7})
						.on("mouseover",highlight)
						.on("mouseout",function(d){});
				
				// adding axes for bar graph
				svg.append("g")
					.attr("class", "x axis")
					.attr("transform", "translate(" + (2.5*pad) + "," + (hei/2 + pad) + ")")
					.call(namesax)
						.selectAll("text")  
							.style("text-anchor", "end")
							.attr("dx", "-.8em")
							.attr("dy", ".15em")
							.attr("transform", function(d) {
								return "rotate(-65)" 
								});	

				svg.append("g")
					.attr("class", "x axis")
					.attr("transform", "translate(" + (pad) + "," + (pad) + ")")
					.call(yaxis);					
					
				// adding dashed lines for time line
				var a_years = d3.range(2000,2016);
				gtime.selectAll(".dashed")
						.data(a_years)
						.enter()
						.append("line")
							.attr("class","dashed")
							.attr("x1",function(d){return xscale(d);})
							.attr("x2",function(d){return xscale(d);})
							.attr("y1",0)
							.attr("y2",hei/2)
							.attr("stroke","grey")
							.attr("stroke-width",2)
							.attr("opacity",0.5)
							.style("stroke-dasharray",("5,2,2,2"));	
				// adding axis
				svg.append("g")
					.attr("class", "x axis")
					.attr("transform", "translate(" + (pad) + "," + (hei+pad+padmid) + ")")
					.call(xaxis);	
			})
			})
			})
				
			function highlight(p) {
				boules.transition()
					.attr("opacity",function(d){
						var oo;
						if (d.short==p.short) {oo=0.9}
						else {oo=0.2}
						if (d.plac==13) {oo=0}
						return oo ;	});
				playline.transition()
					.attr("opacity",function(d){
						return (d.name==p.short)?0.9:0.2 });
				
				barpl.transition(200)
					.attr("y",hei/2)
					.attr("height",0)
				barpl.transition(200).delay(200)
					.attr("y",function(d){
						var yy ;
						if (d.plac==13 || d.Year!=p.Year) {yy=hei/2}
						else {yy=yscale(d.plac)}
						return yy;})
					.attr("height",function(d){
						var hh;
						if (d.plac==13 || d.Year!=p.Year) {hh=0}
						else {hh=hei/2-yscale(d.plac)}
						return hh ;})
				gtitle.text("Placings in "+p.Year)
			}	
						
								
			
						
		</script>

