    <div id='chart2168532d6bd3' class='rChart nvd3'></div>  
    
    <script type='text/javascript'>
 $(document).ready(function(){
      drawchart2168532d6bd3()
    });
    function drawchart2168532d6bd3(){  
      var opts = {
 "dom": "chart2168532d6bd3",
"width":    800,
"height":    400,
"x": "date",
"y": "close",
"y2": "volume",
"type": "linePlusBarChart",
"id": "chart2168532d6bd3" 
},
        data = [
 {
 "date":  13516,
"open":  86.29,
"high":  86.58,
"low":   81.9,
"close":   83.8,
"volume": 4.4226e+07,
"adjusted":  82.04 
},
