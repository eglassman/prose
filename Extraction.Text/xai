<div id="graph"></div>

<div id="controls">
  <button onclick="step();">Step</button>
  <button onclick="changeK(1);">Increase K</button>
  <button onclick="changeK(-1);">Decrease K</button>

  <table id="stats-table">
    <tr>
      <td>
        $∣ \{ x_i : {{x_i}^{t-1}∈c_k , {x_i}^{t}∉c_k } \} ∣$
      </td>
      <td>
        $∑↙{k=1}↖K ∑↙{x_i∈c_k}  {‖ x_i - μ_k ‖^2}$
      </td>
      <!--
      <td>
        $\ov{(m_i-x)}∣x∈S_i$
      </td>
      <td>
        $\ov{(m_i-m_j)}$
      </td>
      -->
    </tr>
  </table>
