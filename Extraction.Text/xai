           var svg = d3.select("body")
                    .append("svg")
				    .attr("width", 400)
                    .attr("height", 380);
                
            d3.csv("fruits-by-race-uploaded-to-gist-FINAL.csv", function(data) {
				
                data.sort(function(a, b) {return d3.descending(a.Apples, b.Apples);});
                
                var rects = svg.selectAll("rect")
					.data(data)
					.enter()
				    .append("rect");
                    
                rects.attr("x", 210)
					.attr("y", function(d, i) {return i * 30;})
					.attr("width", function(d) {return d.Apples * 3;})
					.attr("height", 25)
                                      
                    .append("title")
                    .text(function(d) {return d.RaceEthnicity + "'s annual consumption of APPLES, in retail pounds, is " + d.Apples;});      
            
                svg.selectAll("text")
					.data(data)
					.enter()
					.append("text")
					.text(function(d){return d.RaceEthnicity;})
					.attr("x", 200)
					.attr("y", function(d, i){return i * 30 + 17;})
                	.attr({"fill": "#8b8970",	"font-size": "12px", "text-anchor": "end"})});

        </script>
