var h = {
  'n': [8,11,15,19,22,26,29,33,37],
  'g': 1.20,
  'w': 0.39,
}
/*var h = {
  'n': [4,8,11,15,18,22,25,29,32,36],
  'g': 1.20,
  'w': 0.73,
}*/

/*CZ Senate*/
