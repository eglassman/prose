    <script src="http://d3js.org/d3.v3.min.js"></script>
    <script src="hermite.js"></script>
    <style>
        .panel {
            fill: black;
        }

        canvas {
            display: none;
        }
        .slider {
            width: 400px;
        }

        #phosphene-image {
            width: 400px;
        }
