            circle {
                fill: orange;
                stroke-width: 3px;
                stroke: orange;
                opacity: 0.3;
                stroke-opacity: 0.8;
                
            }
			circle.clickable:hover {
				fill: yellow;
                stroke: black;
                opacity: 0.7;
			}
			g.axis path, g.axis line {
				stroke: green;
				fill: none;
				shape-rendering: crispEdges;
			}
			g.axis text {
				font-family: Arial;
				font-size: 14px;
				opacity: 0.5;
				text-anchor: left;
			}
			g.tick {
				fill: black;
				opacity: 0.5;
				text-anchor: left;
			}
			g.axis.y line, g.axis.y path  {
				stroke: orange;
				opacity: 0.5;
			}
            #databox p {
