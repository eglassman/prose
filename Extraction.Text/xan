	   {"x": 7, "y": 4.82},
	   {"x": 5, "y": 5.68}
	 ];

	var width = 700,
		height = 400; 

	var xScale = d3.scale.linear()
		.range([0,width])
		.domain([2,20]);

	var yScale = d3.scale.linear()
