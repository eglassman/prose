 		 var width = 500;
 		 var height = 350;

    	var svg = d3.select("#bar_Chart")
      .append("svg")
      .attr("width", width + 50)
 		 	.attr("height", height + 50);
      
       var bar_Chart1 = 
     svg.append("g").attr("class", "bar1");
 		 
 		 bar_Chart1.append("rect")
            	  .attr("width", "100%")
            	  .attr("height", "100%")
            	  .attr("stroke", "#999")
 		 		  .attr("fill", "none");
    
