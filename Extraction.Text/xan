            <!-- $Design Practices -->
            <ul class="design-list">
              <li class="list__head"><h3 class="list__title">Design Practices</h3></li>
              <li>
                <label class="control checkbox">
                  <input type="checkbox">
                  <span class="control-indicator"></span>
                  Color profile set as RGB
                </label>
              </li>
