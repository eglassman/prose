    <script>
var chartParams = {
 "dom": "chart77b7203f5527",
"width":    700,
"height":    400,
"data": {
 "1288152000": 4,
"1288324800": 8,
"1288411200": 8,
"1288756800": 2,
"1288929600": 10,
"1289278800": 4,
"1290056400": 3,
