                <rect id="before" fill="url(#before_pattern)" x="0" y="0" width="535" height="500"/>
                <rect id="after" fill="url(#after_pattern)" x="0" y="0" width="535" height="500"/>
                <rect id="overlay" fill="transparent"  x="-20" y="0" width="575" height="500"/>
