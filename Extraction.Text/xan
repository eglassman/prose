  }).setView([41.0252, 28.9950], 11);

  // Disable scroll zoom handler
  map.scrollWheelZoom.disable();

  // Silently enables scrollwheel zoom 3 seconds after a user hovers over the map
  $('.timer').hover(function() {
    setTimeout(function(){
